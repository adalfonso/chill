{
  "name": "chill",
  "version": "0.0.0",
  "description": "DIY Home Media Server",
  "type": "module",
  "author": "Anthony Dalfonso <anthonyjdalfonso@gmail.com>",
  "scripts": {
    "dev": "npx nodemon --watch './server' -e mts --exec 'ts-node-esm ./server/index.dev.mts'",
    "build": "vite build && tsc --project tsconfig.build.json",
    "start": "npm run build && node ./dist/server/index.mjs",
    "docker": "docker-compose up --build",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:publish": "docker build -t adalfonso/chill:latest . && docker push adalfonso/chill:latest",
    "check": "tsc --noEmit && tsc -p tsconfig.build.json --noEmit && npm run lint && jest",
    "lint": "eslint ./{server,client,common}/**/*.{mts,ts,tsx} --quiet",
    "test": "jest --coverage --watchAll"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^0.26.1",
    "bson": "^4.7.2",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.13.0",
    "mongoose": "^6.8.3",
    "music-metadata": "^7.13.3",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "react": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "sharp": "^0.30.7",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@redux-devtools/core": "^3.13.1",
    "@typegoose/typegoose": "^10.0.0",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.45",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth2": "^0.1.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/sharp": "^0.31.1",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.1",
    "concurrently": "^7.6.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.27.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.57.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  }
}
