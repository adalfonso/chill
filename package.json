{
  "name": "chill",
  "version": "0.0.0",
  "description": "DIY Home Media Server",
  "author": "Anthony Dalfonso <anthonyjdalfonso@gmail.com>",
  "scripts": {
    "autogen": "npx mtgen ./server/models/*.ts",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "webpack --config=configs/webpack/client-prod.config.js",
    "build:dev": "rimraf dist && webpack --config configs/webpack/server-dev.config.js",
    "build:dev-watch": "webpack --watch --config configs/webpack/server-dev.config.js",
    "build:server": "webpack --config=configs/webpack/server-prod.config.js",
    "check": "tsc && npm run lint && jest -c=./config/jest.config.js",
    "docker": "docker-compose up --build",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:publish": "docker build -t adalfonso/chill:latest . && docker push adalfonso/chill:latest",
    "lint": "eslint ./{server,client,common}/**/*.{ts,tsx} -c ./configs/.eslintrc.js --quiet",
    "start": "npm run build && node dist/server/index.js",
    "start:dev": "npm run build:dev && concurrently \"npm run build:dev-watch\" \"nodemon dist/server/index\"",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.26.1",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.4",
    "music-metadata": "^7.12.3",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "react": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.3.1",
    "sharp": "^0.30.4",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@redux-devtools/core": "^3.12.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.23",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth2": "^0.1.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.21.1",
