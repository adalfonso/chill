{
  "name": "chill",
  "version": "0.0.0",
  "description": "DIY Home Media Server",
  "type": "module",
  "author": "Anthony Dalfonso <anthonyjdalfonso@gmail.com>",
  "scripts": {
    "dev": "npx nodemon --watch './server' -e mts --exec 'ts-node-esm ./server/index.dev.mts'",
    "build": "vite build && tsc --project tsconfig.build.json",
    "start": "npm run build && node ./dist/server/index.mjs",
    "docker": "docker-compose up --build",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:publish": "docker build -t adalfonso/chill:latest . && docker push adalfonso/chill:latest",
    "check": "tsc --noEmit && npm run lint && jest -c=./config/jest.config.js",
    "lint": "eslint ./{server,client,common}/**/*.{ts,tsx} --quiet",
    "autogen": "npx mtgen ./server/models/**/*",
    "test": "jest --coverage --watchAll"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.26.1",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.5.0",
    "mongoose": "^6.8.3",
    "music-metadata": "^7.12.3",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "react": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.3.1",
    "sharp": "^0.30.4",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@redux-devtools/core": "^3.12.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.23",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth2": "^0.1.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "concurrently": "^7.6.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.21.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "mongoose-tsgen": "^9.0.5",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.57.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "vite": "^4.0.4"
  }
}
